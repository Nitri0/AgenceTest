body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

.angular-google-map-container 
  position: relative
  top: 0
  bottom: 0
  right: 0
  left: 0
  width: 100%
  height: 300px
  margin-top: 30px

.good
  background-color: #009966

.moderate
  background-color: #ffde33

.unhealthy-for-groups
  background-color: #ff9933

.unhealthy
  background-color: #cc0033

.very-unhealthy
  background-color: #660099

.hazardous
  background-color: #7e0023

.color-bar
    min-width: 20px
    height: 100%
    content: close-quote
    position: absolute
    border-radius: 5px

.text-info
  margin-left: 30px